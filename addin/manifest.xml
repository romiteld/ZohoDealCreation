<?xml version='1.0' encoding='utf-8'?>
<ns0:OfficeApp xmlns:ns0="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:ns2="http://schemas.microsoft.com/office/mailappversionoverrides" xmlns:ns3="http://schemas.microsoft.com/office/mailappversionoverrides/1.1" xmlns:ns4="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="MailApp">
  
  <ns0:Id>d2422753-f7f6-4a4a-9e1e-7512f37a50e5</ns0:Id>
  <ns0:Version>1.5.0.4</ns0:Version>
  <ns0:ProviderName>The Well Recruiting Solutions</ns0:ProviderName>
  <ns0:DefaultLocale>en-US</ns0:DefaultLocale>
  <ns0:DisplayName DefaultValue="The Well - Send to Zoho" />
  <ns0:Description DefaultValue="Process recruitment emails and automatically create candidate records in Zoho CRM." />
  <ns0:IconUrl DefaultValue="https://well-intake-api.wittyocean-dfae0f9b.eastus.azurecontainerapps.io/icon-32.png" />
  <ns0:HighResolutionIconUrl DefaultValue="https://well-intake-api.wittyocean-dfae0f9b.eastus.azurecontainerapps.io/icon-80.png" />
  <ns0:SupportUrl DefaultValue="https://thewell.solutions/" />
  
  <ns0:AppDomains>
    <ns0:AppDomain>well-intake-api-dnajdub4azhjcgc3.z03.azurefd.net</ns0:AppDomain>
    <ns0:AppDomain>addin.emailthewell.com</ns0:AppDomain>
    <ns0:AppDomain>well-intake-api.wittyocean-dfae0f9b.eastus.azurecontainerapps.io</ns0:AppDomain>
    <ns0:AppDomain>*.azurefd.net</ns0:AppDomain>
  </ns0:AppDomains>
  
  <ns0:Hosts>
    <ns0:Host Name="Mailbox" />
  </ns0:Hosts>
  
  <ns0:Requirements>
    <ns0:Sets DefaultMinVersion="1.14">
      <ns0:Set Name="Mailbox" />
    </ns0:Sets>
  </ns0:Requirements>
  
  <ns0:FormSettings>
    <ns0:Form xsi:type="ItemRead">
      <ns0:DesktopSettings>
        <ns0:SourceLocation DefaultValue="https://well-intake-api.wittyocean-dfae0f9b.eastus.azurecontainerapps.io/taskpane.html?v=1.5.0.3" />
        <ns0:RequestedHeight>250</ns0:RequestedHeight>
      </ns0:DesktopSettings>
    </ns0:Form>
  </ns0:FormSettings>
  
  <ns0:Permissions>ReadWriteItem</ns0:Permissions>
  
  <ns0:Rule xsi:type="RuleCollection" Mode="Or">
    <ns0:Rule xsi:type="ItemIs" ItemType="Message" FormType="Read" />
  </ns0:Rule>
  
  <ns0:DisableEntityHighlighting>false</ns0:DisableEntityHighlighting>
  
  <ns2:VersionOverrides xsi:type="VersionOverridesV1_0">
    <ns3:VersionOverrides xsi:type="VersionOverridesV1_1">
      <ns3:Requirements>
        <ns4:Sets DefaultMinVersion="1.14">
          <ns4:Set Name="Mailbox" />
        </ns4:Sets>
      </ns3:Requirements>
      
      <ns3:Hosts>
        <ns3:Host xsi:type="MailHost">
          <ns3:DesktopFormFactor>
            <ns3:FunctionFile resid="Commands.Url" />
            
            <ns3:ExtensionPoint xsi:type="MessageReadCommandSurface">
              <ns3:OfficeTab id="TabDefault">
                <ns3:Group id="msgReadGroup">
                  <ns3:Label resid="GroupLabel" />
                  <ns3:Control xsi:type="Button" id="msgReadOpenButton">
                    <ns3:Label resid="TaskpaneButton.Label" />
                    <ns3:Supertip>
                      <ns3:Title resid="TaskpaneButton.Label" />
                      <ns3:Description resid="TaskpaneButton.Tooltip" />
                    </ns3:Supertip>
                    <ns3:Icon>
                      <ns4:Image size="16" resid="Icon.16x16" />
                      <ns4:Image size="32" resid="Icon.32x32" />
                      <ns4:Image size="80" resid="Icon.80x80" />
                    </ns3:Icon>
                    <ns3:Action xsi:type="ShowTaskpane">
                      <ns3:SourceLocation resid="Taskpane.Url" />
                    </ns3:Action>
                  </ns3:Control>
                </ns3:Group>
              </ns3:OfficeTab>
            </ns3:ExtensionPoint>
          </ns3:DesktopFormFactor>
        </ns3:Host>
      </ns3:Hosts>
      
      <ns3:Resources>
        <ns4:Images>
          <ns4:Image id="Icon.16x16" DefaultValue="https://well-intake-api.wittyocean-dfae0f9b.eastus.azurecontainerapps.io/icon-16.png" />
          <ns4:Image id="Icon.32x32" DefaultValue="https://well-intake-api.wittyocean-dfae0f9b.eastus.azurecontainerapps.io/icon-32.png" />
          <ns4:Image id="Icon.80x80" DefaultValue="https://well-intake-api.wittyocean-dfae0f9b.eastus.azurecontainerapps.io/icon-80.png" />
        </ns4:Images>
        
        <ns4:Urls>
          <ns4:Url id="Commands.Url" DefaultValue="https://well-intake-api.wittyocean-dfae0f9b.eastus.azurecontainerapps.io/commands.html?v=1.5.0.4" />
          <ns4:Url id="Taskpane.Url" DefaultValue="https://well-intake-api.wittyocean-dfae0f9b.eastus.azurecontainerapps.io/taskpane.html?v=1.5.0.4" />
        </ns4:Urls>
        
        <ns4:ShortStrings>
          <ns4:String id="GroupLabel" DefaultValue="The Well" />
          <ns4:String id="TaskpaneButton.Label" DefaultValue="Send to Zoho" />
        </ns4:ShortStrings>
        
        <ns4:LongStrings>
          <ns4:String id="TaskpaneButton.Tooltip" DefaultValue="Process this email and create candidate records in Zoho CRM" />
        </ns4:LongStrings>
      </ns3:Resources>
    </ns3:VersionOverrides>
  </ns2:VersionOverrides>
</ns0:OfficeApp>