{
  "routes": [
    {
      "route": "/.auth/login/*",
      "statusCode": 404
    },
    {
      "route": "/.auth/logout",
      "statusCode": 404
    },
    {
      "route": "/.auth/*",
      "statusCode": 404
    },
    {
      "route": "/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{css,scss,js,png,gif,ico,jpg,svg,xml,json,html}"]
  },
  "globalHeaders": {
    "Cache-Control": "no-cache, no-store, must-revalidate",
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "SAMEORIGIN",
    "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://appsforoffice.microsoft.com https://*.azurefd.net; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; font-src 'self'; connect-src 'self' https://well-intake-api.wittyocean-dfae0f9b.eastus.azurecontainerapps.io https://*.office.com https://*.azurefd.net",
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
    "Access-Control-Allow-Headers": "Content-Type, Authorization, X-API-Key"
  },
  "responseOverrides": {
    "401": {
      "rewrite": "/index.html"
    },
    "403": {
      "rewrite": "/index.html"
    }
  }
}