{
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/9188040d-6c67-4c5b-b112-36a304b66dad/v2.0",
          "clientIdSettingName": "AZURE_CLIENT_ID",
          "clientSecretSettingName": "AZURE_CLIENT_SECRET"
        },
        "login": {
          "disableWWWAuthenticate": false
        }
      }
    }
  },
  "routes": [
    {
      "route": "/.auth/logout",
      "statusCode": 302,
      "redirect": "/.auth/logout/complete"
    },
    {
      "route": "/.auth/logout/complete",
      "statusCode": 302,
      "redirect": "/logout.html"
    }
  ]
}