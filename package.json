{
  "name": "the-well-outlook-addin",
  "version": "2.0.0.26",
  "description": "The Well Recruiting Solutions - Outlook Add-in for Zoho CRM Integration",
  "main": "addin/taskpane.js",
  "scripts": {
    "validate": "office-addin-manifest validate addin/manifest.xml",
    "validate:json": "office-addin-manifest validate addin/manifest.json",
    "validate:all": "npm run validate && npm run validate:json",
    "convert": "npx office-addin-manifest-converter convert addin/manifest.xml",
    "package": "npm run package:json && npm run package:xml",
    "package:json": "cd addin && zip -r ../dist/the-well-addin-unified.zip manifest.json icon-*.png",
    "package:xml": "cd addin && zip -r ../dist/the-well-addin-xml.zip manifest.xml icon-*.png *.html *.js",
    "serve": "python3 -m http.server 8080 --directory addin",
    "test": "npm run validate:all",
    "deploy": "npm run validate:all && npm run package"
  },
  "keywords": [
    "outlook",
    "office",
    "addin",
    "zoho",
    "crm",
    "recruitment",
    "email"
  ],
  "author": "The Well Recruiting Solutions",
  "license": "UNLICENSED",
  "devDependencies": {
    "office-addin-manifest": "^1.13.6",
    "office-addin-manifest-converter": "^0.4.1",
    "@microsoft/teams-manifest": "^0.1.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thewell/outlook-addin"
  },
  "bugs": {
    "url": "https://github.com/thewell/outlook-addin/issues"
  },
  "homepage": "https://thewell.solutions"
}