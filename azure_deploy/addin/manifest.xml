<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- Outlook Add-in Manifest for The Well Recruiting -->
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:mailapp="http://schemas.microsoft.com/office/mailappversionoverrides/1.0" xsi:type="MailApp">
    <Id>d2422753-f7f6-4a4a-9e1e-7512f37a50e5</Id>
    <Version>1.0.0.0</Version>
    <ProviderName>The Well Recruiting Solutions</ProviderName>
    <DefaultLocale>en-US</DefaultLocale>
    <DisplayName DefaultValue="Send to Zoho"/>
    <Description DefaultValue="Process candidate emails and create records in Zoho CRM with one click."/>
    <IconUrl DefaultValue="https://well-intake-api.azurewebsites.net/icon-32.png"/>
    <HighResolutionIconUrl DefaultValue="https://well-intake-api.azurewebsites.net/icon-80.png"/>
    <SupportUrl DefaultValue="https://thewell.solutions/"/>
    <AppDomains>
        <AppDomain>https://well-intake-api.azurewebsites.net</AppDomain>
    </AppDomains>
    <Hosts><Host Name="Mailbox"/></Hosts>
    <Requirements><Sets DefaultMinVersion="1.3"><Set Name="Mailbox"/></Sets></Requirements>
    <FormSettings><Form xsi:type="ItemRead"><DesktopSettings><SourceLocation DefaultValue="https://well-intake-api.azurewebsites.net/placeholder.html"/><RequestedHeight>250</RequestedHeight></DesktopSettings></Form></FormSettings>
    <Permissions>ReadWriteMailbox</Permissions>
    <Rule xsi:type="RuleCollection" Mode="Or"><Rule xsi:type="ItemIs" ItemType="Message" FormType="Read"/></Rule>
    <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides" xsi:type="VersionOverridesV1_0">
        <Requirements><bt:Sets DefaultMinVersion="1.3"><bt:Set Name="Mailbox"/></bt:Sets></Requirements>
        <Hosts>
            <Host xsi:type="MailHost">
                <DesktopFormFactor>
                    <FunctionFile resid="FunctionFile.Url"/>
                    <ExtensionPoint xsi:type="MessageReadCommandSurface">
                        <OfficeTab id="TabDefault">
                            <Group id="msgReadGroup">
                                <Label resid="GroupLabel"/>
                                <Control xsi:type="Button" id="msgReadOpenButton">
                                    <Label resid="TaskpaneButton.Label"/>
                                    <Supertip><Title resid="TaskpaneButton.Label"/><Description resid="TaskpaneButton.Tooltip"/></Supertip>
                                    <Icon>
                                        <bt:Image size="16" resid="Icon.16x16"/>
                                        <bt:Image size="32" resid="Icon.32x32"/>
                                        <bt:Image size="80" resid="Icon.80x80"/>
                                    </Icon>
                                    <Action xsi:type="ExecuteFunction">
                                        <FunctionName>processEmail</FunctionName>
                                    </Action>
                                </Control>
                            </Group>
                        </OfficeTab>
                    </ExtensionPoint>
                </DesktopFormFactor>
            </Host>
        </Hosts>
        <Resources>
            <bt:Images>
                <bt:Image id="Icon.16x16" DefaultValue="https://well-intake-api.azurewebsites.net/icon-16.png"/>
                <bt:Image id="Icon.32x32" DefaultValue="https://well-intake-api.azurewebsites.net/icon-32.png"/>
                <bt:Image id="Icon.80x80" DefaultValue="https://well-intake-api.azurewebsites.net/icon-80.png"/>
            </bt:Images>
            <bt:Urls>
                <bt:Url id="FunctionFile.Url" DefaultValue="https://well-intake-api.azurewebsites.net/commands.html"/>
            </bt:Urls>
            <bt:ShortStrings>
                <bt:String id="GroupLabel" DefaultValue="The Well Recruiting"/>
                <bt:String id="TaskpaneButton.Label" DefaultValue="Send to Zoho"/>
            </bt:ShortStrings>
            <bt:LongStrings>
                <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Sends this email to the intake API to create records in Zoho CRM."/>
            </bt:LongStrings>
        </Resources>
    </VersionOverrides>
</OfficeApp>