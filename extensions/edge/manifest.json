{
  "manifest_version": 3,
  "name": "The Well â€“ Send to Zoho (Edge)",
  "description": "Quickly send the current Outlook Web email to The Well intake API to create Zoho records.",
  "version": "0.1.0",
  "minimum_edge_version": "109",
  "action": {
    "default_title": "Send to Zoho",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "identity"
  ],
  "host_permissions": [
    "https://outlook.office.com/*",
    "https://outlook.office365.com/*",
    "https://*.office.com/*",
    "https://well-intake-api.wittyocean-dfae0f9b.eastus.azurecontainerapps.io/*",
    "https://login.microsoftonline.com/*",
    "https://graph.microsoft.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://outlook.office.com/*",
        "https://outlook.office365.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
  }
}


